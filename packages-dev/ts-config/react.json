{
  "display": "React",
  "extends": [
    "./base.json",
    "./node.json",
    "./app.json"
  ],
  "compilerOptions": {
    "jsx": "preserve",
    "noEmit": true,
    "moduleResolution": "bundler",
    "incremental": true,
