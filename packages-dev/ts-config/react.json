{
  "display": "React",
  "extends": [
    "./app.json"
  ],
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "dom",
      "es2015",
      "es2017"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "strict": true,
    "allowJs": false,
    "allowSyntheticDefaultImports": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noUncheckedIndexedAccess": true,
    "declaration": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "jsx": "react",
    "baseUrl": "./",
    "outDir": "lib",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ]
  }
}
