{
  "name": "micro-util-ts",
  "version": "1.8.3",
  "description": "一个常用的工具库",
  "keywords": [
    "micro",
    "utils",
    "micro-utils",
    "debounce",
    "throttle",
    "clone",
    "cloneDeep",
    "Cookie",
    "draggable",
    "LocalStorageHelper",
    "localStorage"
  ],
  "license": "ISC",
  "main": "dist/index",
  "author": "Li Yong",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Not-have/micro-tools/tree/main/packages-utils"
  },
  "bugs": {
    "url": "https://github.com/Not-have/micro-tools/issues"
  },
  "scripts": {
    "build:webpack": "npx rimraf lib && webpack --config webpack.config.js",
    "build:tsc": "npx rimraf dist && tsc",
    "build": "npm run build:webpack && npm run build:tsc",
    "start": "tsc --watch",
    "start:webpack": "webpack --watch",
    "publish:build": "npm run build:tsc && npm publish",
    "clear": "npx rimraf node_modules && npx rimraf dist && npx rimraf lib",
    "prepublishOnly": "npm run build:tsc && npm run build"
  },
  "devDependencies": {
    "fork-ts-checker-webpack-plugin": "^9.0.2",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "webpack": "^5.90.3",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "resize-observer-polyfill": "^1.5.1"
  }
}