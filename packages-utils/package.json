{
  "name": "@mt-kit/utils",
  "version": "1.4.0",
  "description": "一个常用的工具库",
  "keywords": [
    "micro",
    "utils",
    "micro-utils",
    "debounce",
    "throttle",
    "clone",
    "cloneDeep",
    "Cookie",
    "draggable",
    "LocalStorageHelper",
    "localStorage"
  ],
  "homepage": "https://not-have.github.io/micro-tools/",
  "bugs": {
    "url": "https://github.com/Not-have/micro-tools/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Not-have/micro-tools/tree/main/packages-utils"
  },
  "license": "ISC",
  "author": "Li Yong",
  "scripts": {
    "build:webpack": "rm -fr lib && webpack --config webpack.config.js",
    "build:tsc": "rm -fr dist && tsc",
    "build": "pnpm run build:webpack && pnpm run build:tsc",
    "dev": "tsc --watch & webpack --watch",
    "clear": "rm -fr node_modules && rm -fr dist && rm -fr lib",
    "prepublishOnly": "pnpm run build"
  },
  "main": "./lib/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "imports": {
    "#/*": "./src/*"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./lib/index.js"
    }
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "devDependencies": {
    "@types/node": "catalog:",
    "fork-ts-checker-webpack-plugin": "catalog:",
    "ts-loader": "catalog:",
    "typescript": "catalog:",
    "webpack": "catalog:",
    "webpack-cli": "catalog:"
  }
}
